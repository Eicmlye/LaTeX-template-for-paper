{
  "editor.tabSize": 4,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.indentSize": "tabSize",
  "editor.foldingStrategy": "indentation",
  "editor.fontSize": 14,
  "editor.fontFamily": "'Courier New', Consolas, monospace",
  "3dpreview.backgroundColor": "#FFFFFF",
  "latex-workshop.latex.tools": [
    {
        "name": "xelatex",
        "command": "xelatex",
        "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "-xelatex",
            "-outdir=%OUTDIR%",
            "%DOC%"
        ],
        "env": {}
    }
  ],
  "latex-workshop.latex.recipes": [
      {
          "name": "xelatex",
          "tools": [
              "xelatex",
          ]
      }
  ],
//   "vscode-pydata-viewer.pythonPath": "Scripts/python.exe"  // must use absolute path
}